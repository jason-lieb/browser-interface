{
  "name": "Browser Interface",
  "description": "A browser interface for saving windows to the local filesystem.",
  "manifest_version": 3,
  "version": "1.2.0",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "action": {
    "default_title": "Browser Interface",
    "default_icon": "./icons/icon16.png"
  },
  "background": {
    "service_worker": "service-worker/background.ts"
  },
  "options_ui": {
    "page": "browser-interface.html",
    "open_in_tab": true
  },
  "permissions": ["tabs", "storage"],
  "minimum_chrome_version": "88"
}
